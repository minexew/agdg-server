{
  "services": {
    "management_console": {
      "class":      "IManagementConsole",
      "listenPort": "9001",
      "allowedIPs": ["127.0.0.1", "::1"]
    },
    "login": {
      "class":      "ILoginServer",
      "db":         "IJsonLoginDB",
      "listenPort": "9002",
      "forceAnonymousLogin": "false",
      "serverName": "AGDG MMO Official",
      "realms":     [{"name": "AGDG MMO Test Realm", "url": "ws://localhost:9003"}]
    },
    "test_realm": {
      "class":      "IRealmServer",
      "db":         "IJsonRealmDB",
      "worldDir":   "world",
      "listenPort": "9003"
    }
  },
  "contentOutputDir": "content"
}
